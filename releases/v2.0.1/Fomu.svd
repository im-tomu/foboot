                <size>0x24</size>
                <usage>registers</usage>
            </addressBlock>
        </peripheral>
        <peripheral>
            <name>LXSPI</name>
            <baseAddress>0xE0007800</baseAddress>
            <groupName>LXSPI</groupName>
